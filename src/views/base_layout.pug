doctype html
html
  head
    title ServiceMart
    meta(name="viewport" content="width=device-width, initial-scale=1")
    link(href="css/normalize.css", rel="stylesheet")
    link(href="css/modal.min.css", rel="stylesheet")
    link(href="//cdnjs.cloudflare.com/ajax/libs/x-editable/1.5.0/jquery-editable/css/jquery-editable.css",rel="stylesheet")
    link(href="css/collapse.min.css", rel="stylesheet")
    link(href="css/carousel.min.css", rel="stylesheet")
    link(href="css/nav.min.css", rel="stylesheet")
    link(href="css/pagination.min.css", rel="stylesheet")
    link(href="css/dropdown.min.css", rel="stylesheet")
    link(href="https://fonts.googleapis.com/css?family=Arsenal", rel="stylesheet")
    link(href="css/font-awesome.min.css", rel="stylesheet")
    link(href="css/skeleton.css", rel="stylesheet")
    block styles
    link(href="css/main.min.css", rel="stylesheet")
    script(src="js/jquery-3.1.1.min.js", type="text/javascript")
  body
    include partials/_modal.pug
    include partials/_sign_up_modal.pug
    //- .service-mart-overlay
    .content-margin
      header.u-full-width.row
        .logo-row-container
          .container.main-container
            .row.logo-row
              .four.columns.mobile-three
                .logo
                  .logo-content
                    a(href="/", style="text-decoration: none; color: black") <b>SERVICE MART</b>
              block loggedInMenu
              
        .menu-container
          .container.menubar-container
            .row
              .twelve.columns
                .hamburger-container.l-hide
                  button.l-hide.button#hamburger-menu-button MENU
                .menu-wrapper.t-hide
                  .menu.dropdown
                    a#dLabel(href="#", class="button", role="button", data-toggle="dropdown", aria-haspopup="true", aria-expanded="false")
                      | Logo Deisgn &nbsp;
                      i.fa.fa-angle-down
                    .dropdown-menu.top-menu-dropdown(aria-labelledby="dLabel")
                      a(role="button") Dropdown
                  .menu
                    a(href="#", class="button") Graphics &amp; Design
                  .menu
                    a(href="#", class="button") Copywriting
                  .menu
                    a(href="#", class="button") Translation
                  .menu
                    a(href="#", class="button") Digital Marketing
                  .menu.extra
                    a(href="#", class="button") Wordpress
                  .menu
                    a(href="#", class="button") Website &amp; Programming
                  .menu
                    a(href="#", class="button") Tech Support
      .mobile-categories-menu.l-hide
          a#mobile-cat-menu-close(href="#", class="button") X Close
          a(href="#", class="button") Logo Deisgn
          a(href="#", class="button") Graphics &amp; Design
          a(href="#", class="button") Copywriting
          a(href="#", class="button") Translation
          a(href="#", class="button") Digital Marketing
          a(href="#", class="button") Wordpress
          a(href="#", class="button") Website &amp; programming
          a(href="#", class="button") Tech Support
          a(href="#", class="button") Lifestyle
      block content
      footer.u-full-width.row
        .container.main-container
          .row
            .three.columns.tablet-two
              .footer-div
                h5
                  i(class="fa fa-signal")
                  | &nbsp;&nbsp;&nbsp;Discover
                ul
                  li How it works
                  li Trust and Quality
                  li Blog
            .three.columns.tablet-two
              .footer-div
                h5
                  i(class="fa fa-building")
                  | &nbsp;&nbsp;&nbsp;Company
                ul
                  li About Us
                  li Terms of service
                  li Privacy Policy
            .three.columns.tablet-two
              .footer-div
                h5
                  i(class="fa fa-search-plus")
                  | &nbsp;&nbsp;&nbsp;Solutions
                ul
                  li Enterprise Solutions
            .three.columns.tablet-two
              .footer-div
                h5
                  i(class="fa fa-phone")
                  | &nbsp;&nbsp;&nbsp;Contact Us
                ul
                  li Service Mart
                  li contact@servicemart.com
          .row.copyright-row
            .twelve.columns
              .copyright
                p
                  | &copy; Company

    script(src="js/masonry.pkgd.js", type="text/javascript")
    script(src="https://cdnjs.cloudflare.com/ajax/libs/x-editable/1.5.0/jquery-editable/js/jquery-editable-poshytip.min.js")
    script(src="js/modal.min.js", type="text/javascript")
    script(src="chart.js/dist/Chart.js")
    script(src="js/collapse.min.js", type="text/javascript")
    script(src="js/carousel.min.js", type="text/javascript")
    script(src="js/tabs.min.js", type="text/javascript")
    script(src="js/dropdown.min.js", type="text/javascript")
    script(src="js/tinymce/jquery.tinymce.min.js", type="text/javascript")
    script(src="js/tinymce/tinymce.min.js", type="text/javascript")
    block scripts
    script(src="js/main.min.js", type="text/javascript")

      
